    "dayjs": "^1.11.6",
    "ethers": "^5.7.0",
    "pinia": "^2.0.23",
    "pinia-orm": "^1.3.0",
    "rlp": "^3.0.0",
    "smart-timeout": "^2.7.1",
    "vue": "^3.2.37",
