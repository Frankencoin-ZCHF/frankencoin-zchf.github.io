{
  "name": "frankencoin",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p build-only",
    "generateAdresses": "node generateAdresses.js",
    "build-storybook": "build-storybook -o ./dist/styleguide/",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "storybook": "start-storybook -p 6006",
    "validate": "npm-run-all eslint stylelint prettier --continue-on-error",
    "fix": "npm-run-all eslint:fix stylelint:fix prettier:fix --continue-on-error",
    "eslint": "eslint . --ext .vue,.js --ignore-path .gitignore",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier '**/!(README).(js|md|vue)' --list-different",
    "prettier:fix": "npm run prettier -- --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
